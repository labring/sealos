{
  "extends": "../../tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["dom", "dom.iterable", "ES2022"],
    "target": "es2022",
    "forceConsistentCasingInFileNames": false,
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./*"],
      "prisma/*": ["prisma/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
