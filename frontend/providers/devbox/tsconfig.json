{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "ES2020",
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "@/*": ["./*"],
      "prisma/*": ["prisma/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "extends": "../../tsconfig.web.json"
}
