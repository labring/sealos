{
  "openapi": "3.0.0",
  "info": {
    "title": "Devbox API",
    "version": "1.0.0",
    "description": ""
  },
  "paths": {
    "/api/v1/createSimpleDevbox": {
      "get": {
        "description": "New endpoint",
        "responses": {
          "200": {
            "description": "New response",
            "content": {
              "application/json": {
                "schema": {
                  "": ""
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Lifecycle"
        ],
        "summary": "创建Devbox",
        "description": "创建一个新的Devbox，即Create",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxForm": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "minLength": 1,
                        "description": "Devbox名称"
                      },
                      "runtimeName": {
                        "type": "string",
                        "enum": [
                          "Debian",
                          "C++",
                          "Rust",
                          "Java",
                          "Go",
                          "Python",
                          "Node.js",
                          ".Net",
                          "C",
                          "PHP"
                        ],
                        "description": "Devbox类型，有以下可选项：Debian，C++，Rust，Java，Go，Python，Node.js，.Net，C，PHP"
                      },
                      "cpu": {
                        "type": "number",
                        "minimum": 0,
                        "default": 2000,
                        "description": "CPU核心数, 使用1000，2000的格式,代表1Core, 2Core"
                      },
                      "memory": {
                        "type": "number",
                        "minimum": 0,
                        "default": 4096,
                        "description": "内存数, 使用2048, 4096,代表2G, 4G"
                      }
                    },
                    "required": [
                      "name",
                      "runtimeName"
                    ]
                  }
                },
                "required": [
                  "devboxForm"
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/releaseDevboxDefault": {
      "post": {
        "tags": [
          "ReleaseAndDeploy"
        ],
        "summary": "Devbox发版",
        "description": "Devbox发版，release，为现有的Devbox发布一个新的版本",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxForm": {
                    "type": "object",
                    "properties": {
                      "devboxName": {
                        "type": "string",
                        "minLength": 1,
                        "description": "Devbox名称"
                      },
                      "tag": {
                        "type": "string",
                        "minLength": 1,
                        "description": "新版本的版本号"
                      },
                      "releaseDes": {
                        "type": "string",
                        "default": "",
                        "description": "新版本的描述"
                      }
                    },
                    "required": [
                      "devboxName",
                      "tag"
                    ]
                  }
                },
                "required": [
                  "devboxForm"
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/getDevboxReleaseListDefault": {
      "get": {
        "tags": [
          "ReleaseAndDeploy"
        ],
        "summary": "获取Devbox版本历史",
        "description": "获取单个Devbox的所有历史版本",
        "parameters": [
          {
            "in": "query",
            "name": "devboxName",
            "description": "Devbox名称",
            "schema": {
              "type": "string",
              "minLength": 1,
              "description": "Devbox名称"
            },
            "required": true
          }
        ]
      }
    },
    "/api/v1/lifecycleDevbox": {
      "post": {
        "tags": [
          "Lifecycle"
        ],
        "summary": "操作Devbox",
        "description": "操作Devbox，Devbox生命周期管理，主要包含开机，关机，重启，冷关机，这些操作都通过这个工具完成",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Devbox名称"
                  },
                  "action": {
                    "type": "string",
                    "enum": [
                      "start",
                      "stop",
                      "restart",
                      "shutdown"
                    ],
                    "description": "操作方式：start为开机，stop为关机，restart为重启，shutdown为冷关机"
                  }
                },
                "required": [
                  "devboxName",
                  "action"
                ]
              }
            }
          }
        },
        "/api/v1/getDevboxListEasyResponse": {
          "get": {
            "tags": [
              "Query"
            ],
            "summary": "获取所有Devbox",
            "description": "获取所有Devbox，得到Devbox的列表，可以知道当前有哪些Devbox"
          }
        }
      }
    },
    "/api/v1/createDevboxPort": {
      "post": {
        "tags": [
          "Port"
        ],
        "summary": "创建Devbox端口",
        "description": "创建Devbox端口，为现有的Devbox创建一个新的端口",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Devbox名称"
                  },
                  "port": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 65535,
                    "description": "新创建端口的端口号"
                  },
                  "protocol": {
                    "type": "string",
                    "enum": [
                      "HTTP",
                      "GRPC",
                      "WS"
                    ],
                    "default": "HTTP",
                    "description": "这个端口的通信方式，有三种通信方式可选：HTTP，GRPC，WS"
                  }
                },
                "required": [
                  "devboxName",
                  "port"
                ]
              }
            }
          }
        }
      }
    },
    "/api/deployDevbox": {
      "post": {
        "tags": [
          "Deploy"
        ],
        "summary": "部署 Devbox",
        "description": "部署 Devbox",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Devbox名称"
                  },
                  "port": {
                    "type": "number",
                    "minimum": 1,
                    "default": 65535,
                    "description": "端口号"
                  },
                  "tag": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Devbox版本号，即Devbox发布版本时所使用的版本号"
                  },
                  "cpu": {
                    "type": "number",
                    "minimum": 0,
                    "default": 2000,
                    "description": "CPU核心数，使用1000、2000，分别代表1核、2核"
                  },
                  "memory": {
                    "type": "number",
                    "minimum": 0,
                    "default": 4096,
                    "description": "内存大小，使用2048、4096，分别代表2G、4G"
                  }
                },
                "required": [
                  "devboxName",
                  "tag"
                ]
              }
            }
          }
        }
      }
    }
  }
}