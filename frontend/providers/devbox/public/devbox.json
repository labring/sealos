{
  "openapi": "3.0.0",
  "info": {
    "title": "Devbox API",
    "version": "1.0.0",
    "description": ""
  },
  "paths": {
    "/api/v1/DevBox/create": {
      "post": {
        "summary": "createDevbox",
        "description": "Use a template to create a new DevBox.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "description": "DevBox Name"
                  },
                  "runtimeName": {
                    "type": "string",
                    "enum": [
                      "Debian",
                      "C++",
                      "Rust",
                      "Java",
                      "Go",
                      "Python",
                      "Node.js",
                      ".Net",
                      "C",
                      "PHP"
                    ],
                    "description": "The name of the template used to create the DevBox, with the following options: Debian, C++, Rust, Java, Go, Python, Node.js, .Net, C, PHP"
                  },
                  "cpu": {
                    "type": "number",
                    "minimum": 0,
                    "default": 2000,
                    "description": "Number of CPU cores, using the format 1000, 2000, representing 1 Core, 2 Core"
                  },
                  "memory": {
                    "type": "number",
                    "minimum": 0,
                    "default": 4096,
                    "description": "Memory size, using the format 2048, 4096, representing 2G, 4G"
                  }
                },
                "required": ["name", "runtimeName"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Devbox created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "description": "Devbox name"
                        },
                        "sshPort": {
                          "type": "number",
                          "description": "SSH port"
                        },
                        "base64PrivateKey": {
                          "type": "string",
                          "description": "Base64 encoded private key"
                        },
                        "userName": {
                          "type": "string",
                          "description": "User name"
                        },
                        "workingDir": {
                          "type": "string",
                          "description": "Working directory"
                        },
                        "domain": {
                          "type": "string",
                          "description": "Domain"
                        }
                      },
                      "required": [
                        "name",
                        "sshPort",
                        "base64PrivateKey",
                        "userName",
                        "workingDir",
                        "domain"
                      ]
                    }
                  },
                  "required": ["data"]
                }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "string"
                    }
                  },
                  "required": ["code", "message"]
                }
              }
            }
          },
          "404": {
            "description": "Template not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "string"
                    }
                  },
                  "required": ["code", "message"]
                }
              }
            }
          },
          "409": {
            "description": "Devbox already exists",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "string"
                    }
                  },
                  "required": ["code", "message"]
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "string"
                    }
                  },
                  "required": ["code", "message"]
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/DevBox/release": {
      "post": {
        "summary": "releaseDevbox",
        "description": "DevBox release, publish a new version for an existing DevBox",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "DevBox name"
                  },
                  "tag": {
                    "type": "string",
                    "minLength": 1,
                    "description": "New tag for DevBox"
                  },
                  "releaseDes": {
                    "type": "string",
                    "default": "",
                    "description": "Description of the new version"
                  }
                },
                "required": ["devboxName", "tag"]
              }
            }
          }
        }
      }
    },
    "/api/v1/DevBox/releases": {
      "get": {
        "summary": "getDevboxreleases",
        "description": "Get all historical versions of a single DevBox",
        "parameters": [
          {
            "in": "query",
            "name": "devboxName",
            "description": "DevBox name",
            "schema": {
              "type": "string",
              "minLength": 1,
              "description": "DevBox name"
            },
            "required": true
          }
        ]
      }
    },
    "/api/v1/DevBox/lifecycle": {
      "post": {
        "summary": "manageDevbox",
        "description": "Managing DevBox, DevBox lifecycle management, mainly including powering on, shutting down, restarting, cold shutdown, all these operations are completed through this tool",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "DevBox name"
                  },
                  "action": {
                    "type": "string",
                    "enum": ["start", "stop", "restart", "shutdown"],
                    "description": "Manage methods: start for powering on, stop for shutting down, restart for restarting, shutdown for cold shutdown"
                  }
                },
                "required": ["devboxName", "action"]
              }
            }
          }
        }
      }
    },
    "/api/v1/DevBox/ports/create": {
      "post": {
        "summary": "createPort",
        "description": "Create DevBox port, create a new port for an existing DevBox",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "DevBox name"
                  },
                  "port": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 65535,
                    "description": "Port number of the newly created port"
                  },
                  "protocol": {
                    "type": "string",
                    "enum": ["HTTP", "GRPC", "WS"],
                    "default": "HTTP",
                    "description": "Communication method for this port, three communication methods available: HTTP, GRPC, WS"
                  }
                },
                "required": ["devboxName", "port"]
              }
            }
          }
        }
      }
    },
    "/api/deployDevbox": {
      "post": {
        "summary": "deployDevbox",
        "description": "Deploy Devbox",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "devboxName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "DevBox name"
                  },
                  "port": {
                    "type": "number",
                    "minimum": 1,
                    "default": 65535,
                    "description": "port number"
                  },
                  "tag": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Devbox release version number"
                  },
                  "cpu": {
                    "type": "number",
                    "minimum": 0,
                    "default": 2000,
                    "description": "Number of CPU cores, using the format 1000, 2000, representing 1 Core, 2 Core"
                  },
                  "memory": {
                    "type": "number",
                    "minimum": 0,
                    "default": 4096,
                    "description": "Memory size, using the format 2048, 4096, representing 2G, 4G"
                  }
                },
                "required": ["devboxName", "tag"]
              }
            }
          }
        }
      }
    },
    "/api/v1/DevBox/list": {
      "get": {
        "tags": ["Query"],
        "summary": "getDevboxList",
        "description": "Get DevBox list, obtain detailed information for each DevBox"
      }
    }
  }
}
