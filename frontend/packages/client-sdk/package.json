{
  "name": "sealos-desktop-sdk",
  "version": "0.1.14",
  "description": "sealos desktop sdk",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./master": {
      "import": "./dist/master.esm.js",
      "require": "./dist/master.js",
      "types": "./dist/master.d.ts"
    },
    "./app": {
      "import": "./dist/app.esm.js",
      "require": "./dist/app.js",
      "types": "./dist/app.d.ts"
    },
    "./service": {
      "import": "./dist/service.esm.js",
      "require": "./dist/service.js",
      "types": "./dist/service.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "master": [
        "./dist/master.d.ts"
      ],
      "app": [
        "./dist/app.d.ts"
      ],
      "service": [
        "./dist/service.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": "lizhenq2009@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@kubernetes/client-node": "^0.18.1",
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/js-cookie": "^3.0.4",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.7.1",
    "@types/uuid": "^9.0.4",
    "rollup": "2.79.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^4.2.3",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@kubernetes/client-node": "^0.18.1"
  }
}