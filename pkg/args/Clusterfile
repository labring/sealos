  creationTimestamp: null
  name: registry
spec:
  data: |
    domain: sealos.hub
    ip: 192.168.64.15
    password: passw0rd
    port: 5000
    username: admin
    data: /var/lib/registry
  path: etc/registry.yml
  strategy: merge

