apiVersion: apps.sealyun.com/v1
kind: Cluster
metadata:
  creationTimestamp: null
  name: default
spec:
  env:
  - Interface=eth.*|en.*|em.*
  - IPIP=Always
  - MTU=1440
  - criData=/var/lib/containerd
  - registryData=/var/lib/registry
  - registryConfig=/etc/registry
  - registryDomain=sealos.hub
  - registryPort=5000
  - registryUsername=admin
  - registryPassword=passw0rd
  hosts:
  - ips:
    - 192.168.64.15
    roles:
    - master
    - amd64
  - ips:
    - 192.168.64.17
    roles:
    - node
    - amd64
  ssh:
    pk: /Users/cuisongliu/.ssh/id_rsa
    port: 22
    user: root

