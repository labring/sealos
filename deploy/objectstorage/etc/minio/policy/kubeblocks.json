{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:GetBucketTagging",
        "s3:ListBucket",
        "s3:ListMultipartUploadParts",
        "s3:PutBucketTagging",
        "s3:PutObject",
        "s3:CreateBucket",
        "s3:DeleteObject",
        "s3:GetBucketLocation",
        "s3:GetBucketPolicy",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::file-migration/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:CreateBucket",
        "s3:DeleteObject",
        "s3:GetBucketLocation",
        "s3:PutObject",
        "s3:GetBucketPolicy",
        "s3:GetBucketTagging",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads",
        "s3:ListMultipartUploadParts",
        "s3:PutBucketTagging"
      ],
      "Resource": [
        "arn:aws:s3:::file-backup/*"
      ]
    }
  ]
}